{
    "source": {
        "include": [
            "src"
        ],
        "exclude": [
            "src/lib"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/api",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/minami"
    },
    "plugins": [
        "plugins/markdown"
    ]
}